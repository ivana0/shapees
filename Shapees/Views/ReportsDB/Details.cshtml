@model Shapees.Models.DatabaseModel.Report

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<head>
    <!--    SCRIPT TO CREATE AND DOWNLAD PDF    -->
    <script type="text/javascript">

    //Create pdf file out of html form elements
    function onClick() {
        var pdf = new jsPDF();

        //nsw education logo
        var nswGovEduLogo = 'data:image/png;base64,'
        pdf.addImage(nswGovEduLogo, 'PNG', 10, 10, 40, 15)

        //add kids uni logo with uni centre
        var kidsunilogowithunicentre = 'data:image/png;base64,'
        pdf.addImage(kidsunilogowithunicentre, 'PNG', 140, 10, 60, 20)

        pdf.setTextColor(0, 0, 0);
        pdf.setFontSize(10)
        pdf.setFontType("normal");
        pdf.text(10, 40, "Date created: " + '@Model.Datecreated.ToString("dd/MM/yyyy")')

        //pdf.setDrawColor(96, 96, 96)
        pdf.setDrawColor(0, 0, 0)
        pdf.setLineWidth(0.1)
        pdf.line(10, 45, 200, 45)        // horizontal line
        //draw blue box
        pdf.setFillColor(32, 178, 170);
        pdf.rect(10, 47, 190, 30, 'F'); // filled square
        //draw title
        //pdf.setDrawColor(255, 255, 255);
        pdf.setTextColor(255, 255, 255);
        pdf.setFontSize(30)
        pdf.setFont("helvetica")
        pdf.text(15, 65, '@Model.Reporttype')

        pdf.setTextColor(0, 0, 0);
        pdf.setFontSize(20);

        pdf.text(10, 90, '@Model.Title')

        pdf.setTextColor(128, 128, 128);
        pdf.setFontType("bold");
        pdf.setFontSize(14)
        pdf.text(10, 105, "Report Information")
        pdf.setDrawColor(0, 0, 0)
        pdf.setLineWidth(0.1)
        pdf.setTextColor(0, 0, 0);
        pdf.line(10, 108, 200, 108)        // horizontal line
        pdf.setTextColor(0, 0, 0);
        pdf.setFontSize(10)
        pdf.setFontType("bold");
        pdf.text(10, 115, "Educator's full name: ")
        pdf.setFontType("normal");
        pdf.text(55, 115, '@Model.AuthorFullName')
        pdf.setFontSize(10)

        pdf.setTextColor(128, 128, 128);
        pdf.setFontType("bold");
        pdf.setFontSize(10)
        pdf.text(10, 122, "About Educator")
        pdf.setTextColor(0, 0, 0);
        var shortbio = '@ViewData["shortbio"]';
        var splitShortBioText = pdf.splitTextToSize(shortbio, 180);
        pdf.setFontType("normal");
        pdf.setFontSize(10)
        pdf.text(15, 128, splitShortBioText)

        pdf.setFontType("bold");
        pdf.text(10, 145, "Child's full name: ")
        pdf.setFontType("normal");
        pdf.text(55, 145, '@Model.ChildFullName')
        pdf.setFontSize(10)

        pdf.setFontType("bold");
        pdf.text(10, 155, "Date of birth: ")
        pdf.setFontType("normal");
        var dob = '@ViewData["childdob"]';
        pdf.text(55, 155, dob)

        pdf.setFontType("bold");
        pdf.text(10, 165, "Room: ")
        pdf.setFontType("normal");
        var room = '@ViewData["room"]';
        pdf.text(55, 165, room)

        pdf.setTextColor(128, 128, 128);
        pdf.setFontType("bold");
        pdf.setFontSize(14)
        pdf.text(10, 180, "Introduction")
        //pdf.setDrawColor(96, 96, 96)
        pdf.setLineWidth(0.1)
        pdf.line(10, 183, 200, 183)        // horizontal line
        var splitInroText = pdf.splitTextToSize('@Model.Subject', 180);
        pdf.setTextColor(0, 0, 0);
        pdf.setFontType('normal')
        pdf.setFontSize(10)
        pdf.text(15, 190, splitInroText)

        //add page
        pdf.addPage()
        pdf.setTextColor(128, 128, 128);
        pdf.setFontType("bold");
        pdf.setFontSize(14)
        pdf.text(10, 20, "Report")
        pdf.setTextColor(0, 0, 0);
        pdf.setFontType("normal");
        pdf.setFontSize(10)
        pdf.setDrawColor(0, 0, 0)
        pdf.setLineWidth(0.1)
        pdf.line(10, 23, 200, 23)        // horizontal line

        var splitText = pdf.splitTextToSize('@Model.Bodytext', 180);
        pdf.text(15, 30, splitText);

        // Output as Data URI
        pdf.output('datauri');
        pdf.save('KidsUniReport.pdf');
    };

    var element = document.getElementById("clickbind");
    element.addEventListener("clickbind", onClick);
    </script>
</head>

<h2>Details</h2>

<div>
    <h4>Report</h4>
    <hr />
    <div>
        <a id="clickbind" onclick="onClick()" href="#"><span style="color:limegreen;" class="glyphicon glyphicon-download" aria-hidden="true"></span>&ensp;Click to Download the Report as PDF</a>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.3/jspdf.min.js"></script>
        <br /><br />
    </div>
    <dl class="dl-horizontal">
        <dt>
            Report type
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Reporttype)
        </dd>
        <dt>
            Author
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AuthorFullName)
        </dd>
        <dt>
            Child
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ChildFullName)
        </dd>
        <dt>
            Date created
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Datecreated)
        </dd>
        <dt>
            Title
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt>
            Introduction
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Subject)
        </dd>
        <dt>
            Report Content
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Bodytext)
        </dd>
    </dl>
</div>
<div>
    &emsp;<a asp-action="Index"><span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>&ensp;Back to Report List</a> &emsp;|&emsp;
    <a asp-action="Edit" asp-route-id="@Model.Reportid"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>&ensp;Edit</a> 
    <br /><br />
</div>

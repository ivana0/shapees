@model IEnumerable<Shapees.Models.Users>
    @{
        ViewData["Title"] = "Users";
        Layout = "~/Views/Shared/UsersLayout.cshtml";
    }

    <h2>Users</h2>

    <table class="table">
        <tr>
            <th>User ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
            <th>User Type</th>
            <th>Last logged in</th>
            <th>Is logged in</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Username)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pass)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.user_type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.last_login)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.is_logged_in)
                </td>
            </tr>
        }
    </table>
